
function up {
  num=$1
  while [ $num -ne 0  ];do
    cd ..
    num=$((num-1))
  done
}

cd

export PATH=$PATH:/usr/local/cuda-10.2/bin
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/usr/local/cuda-10.2/lib64
export PATH=$PATH:/usr/local/NVIDIA-Nsight-Compute-2019.5/
export TERM=screen-256color

# change terminal background colour to something around 17191F and for xournal to something like 25262A
